{
  "name": "arraypress/s3-utilities",
  "description": "Offers utility methods for cleansing and validating input parameters associated with S3 operations.",
  "homepage": "https://github.com/arraypress/s3-utilities/",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\S3\\Utils\\": "src/"
    },
    "files": [
      "src/Functions.php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}